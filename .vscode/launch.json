{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: Main",
      "type": "python",
      "request": "launch",
      "program": "python_scratchpad/src/main.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Vite: Launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/vite_scratchpad"
    }
  ],
}
